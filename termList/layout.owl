<!-- Concept: back to back  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8436"/>
      <skos:prefLabel xml:lang="en">back to back</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">back to back</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout in which the same image is represented in more than one language on each side of a sheet.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout in which the same image is represented in more than one language on each side of a sheet.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to cartographic images.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">back-to-back</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">back-to-back</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="de">back to back</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="de">back to back</rdakit:toolkitLabel>
      <skos:definition xml:lang="de">Ein Layout, in dem das gleiche Bild in mehr als einer Sprache auf jeder Seite eines Blattes dargestellt wird.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="de">Ein Layout, in dem das gleiche Bild in mehr als einer Sprache auf jeder Seite eines Blattes dargestellt wird.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="de">Dieser Terminus trifft auf kartografische Bilder zu.</skos:scopeNote>
      <skos:prefLabel xml:lang="fi">kääntöpuolilla</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">kääntöpuolilla</rdakit:toolkitLabel>
      <skos:definition xml:lang="fi">Layout, jossa sama kuva esitetään useammalla kuin yhdellä kielellä arkin molemmin puolin.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fi">Layout, jossa sama kuva esitetään useammalla kuin yhdellä kielellä arkin molemmin puolin.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fi">Termi koskee kartografisia kuvia.</skos:scopeNote>
      <skos:prefLabel xml:lang="vi">giáp lưng</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="vi">giáp lưng</rdakit:toolkitLabel>
      <skos:definition xml:lang="vi">Sắp đặt trong đó cùng một hình ảnh được trình bày theo hai hoặc nhiều hơn ngôn ngữ trên mỗi mặt của tờ.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="vi">Sắp đặt trong đó cùng một hình ảnh được trình bày theo hai hoặc nhiều hơn ngôn ngữ trên mỗi mặt của tờ.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="vi">Thuật ngữ này áp dụng cho hình ảnh bản đồ.</skos:scopeNote>
      <skos:prefLabel xml:lang="da">back to back</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">back to back</rdakit:toolkitLabel>
      <skos:definition xml:lang="da">Layout hvor det samme billede er repræsenteret ved mere end ét sprog på hver side af et ark.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="da">Layout hvor det samme billede er repræsenteret ved mere end ét sprog på hver side af et ark.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="da">Denne term anvendes til kartografiske billeder.</skos:scopeNote>
      <skos:prefLabel xml:lang="fr">dos à dos</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fr">dos à dos</rdakit:toolkitLabel>
      <skos:definition xml:lang="fr">Disposition dans laquelle la même image est présentée dans plus d’une langue sur chaque côté d’une feuille.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fr">Disposition dans laquelle la même image est présentée dans plus d’une langue sur chaque côté d’une feuille.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fr">Ce terme s’applique aux images cartographiques.</skos:scopeNote>
      <skos:prefLabel xml:lang="no">back to back</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="no">back to back</rdakit:toolkitLabel>
      <skos:definition xml:lang="no">Et oppsett der det samme bildet er gjengitt på mer enn ett språk på hver side av et ark.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="no">Et oppsett der det samme bildet er gjengitt på mer enn ett språk på hver side av et ark.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="no">Dette begrepet gjelder kartografiske bilder.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: bar by bar  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8437"/>
      <skos:prefLabel xml:lang="en">bar by bar</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">bar by bar</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of music displayed in indented paragraphs with all of the parts for each measure written consecutively.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of music displayed in indented paragraphs with all of the parts for each measure written consecutively.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">battuta per battuta</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">battuta per battuta</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="de">Takt für Takt</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="de">Takt für Takt</rdakit:toolkitLabel>
      <skos:definition xml:lang="de">Ein Layout, bei dem Noten in eingerückten Absätzen dargestellt werden, wobei alle Stimmen für jeden Takt nacheinander folgend geschrieben sind.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="de">Ein Layout, bei dem Noten in eingerückten Absätzen dargestellt werden, wobei alle Stimmen für jeden Takt nacheinander folgend geschrieben sind.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="de">Dieser Terminus trifft auf taktile Musikressourcen zu.</skos:scopeNote>
      <skos:prefLabel xml:lang="fi">tahdit peräkkäin</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">tahdit peräkkäin</rdakit:toolkitLabel>
      <skos:definition xml:lang="fi">Layout, joka koostuu sisennetyissä kappaleissa esitetystä nuottikirjoituksesta, ja jossa jokaisen tahdin stemmat on merkitty peräkkäin.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fi">Layout, joka koostuu sisennetyissä kappaleissa esitetystä nuottikirjoituksesta, ja jossa jokaisen tahdin stemmat on merkitty peräkkäin.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fi">Termi koskee taktiileja musiikkiaineistoja.</skos:scopeNote>
      <skos:prefLabel xml:lang="vi">gạch nhịp sau gạch nhịp</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="vi">gạch nhịp sau gạch nhịp</rdakit:toolkitLabel>
      <skos:definition xml:lang="vi">Sắp đặt bao gồm có âm nhạc được hiện trong các đoạn dự kiến với tất cả các phần cho mỗi nhịp được viết liên tiếp.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="vi">Sắp đặt bao gồm có âm nhạc được hiện trong các đoạn dự kiến với tất cả các phần cho mỗi nhịp được viết liên tiếp.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="vi">Thuật ngữ này áp dụng cho tài nguyên âm nhạc xúc giác.</skos:scopeNote>
      <skos:prefLabel xml:lang="da">takt for takt</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">takt for takt</rdakit:toolkitLabel>
      <skos:definition xml:lang="da">Layout bestående af musik vist i indrykkede afsnit med alle stemmerne for hver takt skrevet efter hinanden.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="da">Layout bestående af musik vist i indrykkede afsnit med alle stemmerne for hver takt skrevet efter hinanden.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="da">Denne term anvendes til taktile musik-ressourcer.</skos:scopeNote>
      <skos:prefLabel xml:lang="fr">mesure par mesure</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fr">mesure par mesure</rdakit:toolkitLabel>
      <skos:definition xml:lang="fr">Disposition qui consiste en musique présentée sous forme de paragraphes en retrait dans lesquels toutes les parties figurant dans une même mesure sont écrites l’une à la suite de l’autre.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fr">Disposition qui consiste en musique présentée sous forme de paragraphes en retrait dans lesquels toutes les parties figurant dans une même mesure sont écrites l’une à la suite de l’autre.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fr">Ce terme s’applique aux ressources de musique tactile.</skos:scopeNote>
      <skos:prefLabel xml:lang="no">bar by bar</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="no">bar by bar</rdakit:toolkitLabel>
      <skos:definition xml:lang="no">Et oppsett som består av noter vist i avsnitt med innrykk der alle stemmene for hver takt er skrevet fortløpende.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="no">Et oppsett som består av noter vist i avsnitt med innrykk der alle stemmene for hver takt er skrevet fortløpende.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="no">Dette begrepet gjelder taktile musikkressurser.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: bar over bar  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8438"/>
      <skos:prefLabel xml:lang="en">bar over bar</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">bar over bar</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of measures displayed in units called parallels and that the beginning of each part’s measure is vertically aligned with the beginning of the same measure for the other part or parts.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of measures displayed in units called parallels and that the beginning of each part’s measure is vertically aligned with the beginning of the same measure for the other part or parts.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">battuta sopra battuta</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">battuta sopra battuta</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="de">Takt über Takt</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="de">Takt über Takt</rdakit:toolkitLabel>
      <skos:definition xml:lang="de">Ein Layout, bei dem Noten in Einheiten, sogenannten Parallelen, dargestellt werden, wobei der Beginn eines Takts bei jeder Stimme senkrecht über dem gleichen Takt für jede andere Stimme steht.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="de">Ein Layout, bei dem Noten in Einheiten, sogenannten Parallelen, dargestellt werden, wobei der Beginn eines Takts bei jeder Stimme senkrecht über dem gleichen Takt für jede andere Stimme steht.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="de">Dieser Terminus trifft auf taktile Musikressourcen zu.</skos:scopeNote>
      <skos:prefLabel xml:lang="fi">tahdit allekkain</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">tahdit allekkain</rdakit:toolkitLabel>
      <skos:definition xml:lang="fi">Layout, joka koostuu rinnakkaisissa yksiköissä esitetyistä tahdeista siten, että kunkin stemman tahdin alku on pystysuuntaisesti tasattu toisen stemman tai stemmojen saman tahdin alun kanssa.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fi">Layout, joka koostuu rinnakkaisissa yksiköissä esitetyistä tahdeista siten, että kunkin stemman tahdin alku on pystysuuntaisesti tasattu toisen stemman tai stemmojen saman tahdin alun kanssa.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fi">Termi koskee taktiileja musiikkiaineistoja.</skos:scopeNote>
      <skos:prefLabel xml:lang="vi">gạch nhịp trên gạch nhịp</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="vi">gạch nhịp trên gạch nhịp</rdakit:toolkitLabel>
      <skos:definition xml:lang="vi">Sắp đặt gồm có các nhịp được hiện thành các đơn vị gọi là tương đương bắt đầu mỗi nhịp được sắp hàng dọc với bắt đầu bằng cùng nhịp cho mỗi phần hoặc các phần.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="vi">Sắp đặt gồm có các nhịp được hiện thành các đơn vị gọi là tương đương bắt đầu mỗi nhịp được sắp hàng dọc với bắt đầu bằng cùng nhịp cho mỗi phần hoặc các phần.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="vi">Thuật ngữ này áp dụng cho tài nguyên âm nhạc xúc giác.</skos:scopeNote>
      <skos:prefLabel xml:lang="da">takt over takt</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">takt over takt</rdakit:toolkitLabel>
      <skos:definition xml:lang="da">Layout bestående af musik vist i enheder kaldet paralleller og hvor starten af hver stemmes takt er på linje med starten af den samme takt for den anden stemme eller stemmer.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="da">Layout bestående af musik vist i enheder kaldet paralleller og hvor starten af hver stemmes takt er på linje med starten af den samme takt for den anden stemme eller stemmer.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="da">Denne term anvendes til taktile musik-ressourcer.</skos:scopeNote>
      <skos:prefLabel xml:lang="fr">mesure sur mesure</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fr">mesure sur mesure</rdakit:toolkitLabel>
      <skos:definition xml:lang="fr">Disposition qui consiste en mesures présentées sous forme d’unités appelées parallèles dans lesquelles le début de chaque mesure dans une partie donnée est aligné verticalement avec le début de la même mesure dans la ou les autres parties.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fr">Disposition qui consiste en mesures présentées sous forme d’unités appelées parallèles dans lesquelles le début de chaque mesure dans une partie donnée est aligné verticalement avec le début de la même mesure dans la ou les autres parties.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fr">Ce terme s’applique aux ressources de musique tactile.</skos:scopeNote>
      <skos:prefLabel xml:lang="no">bar over bar</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="no">bar over bar</rdakit:toolkitLabel>
      <skos:definition xml:lang="no">Et oppsett som består av takter som vises i parallelle enheter der begynnelsen takten for hver stemme er på loddrett linje med begynnelsen av samme takten for alle de andre stemmene.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="no">Et oppsett som består av takter som vises i parallelle enheter der begynnelsen takten for hver stemme er på loddrett linje med begynnelsen av samme takten for alle de andre stemmene.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="no">Dette begrepet gjelder taktile musikkressurser.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: both sides  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1006" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8439"/>
      <skos:prefLabel xml:lang="en">both sides</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">both sides</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout that is continued at the same scale on both sides of a sheet or sheets.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout that is continued at the same scale on both sides of a sheet or sheets.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to cartographic images.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">entrambi i lati</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">entrambi i lati</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="de">beidseitig</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="de">beidseitig</rdakit:toolkitLabel>
      <skos:definition xml:lang="de">Ein Layout, das im selben Maßstab auf beiden Seiten eines Blattes fortgesetzt wird.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="de">Ein Layout, das im selben Maßstab auf beiden Seiten eines Blattes fortgesetzt wird.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="de">Dieser Terminus trifft auf kartografische Bilder zu.</skos:scopeNote>
      <skos:prefLabel xml:lang="fi">molemmilla puolilla</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">molemmilla puolilla</rdakit:toolkitLabel>
      <skos:definition xml:lang="fi">Layout, joka jatkuu samassa mittakaavassa arkin tai arkkien molemmilla puolilla.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fi">Layout, joka jatkuu samassa mittakaavassa arkin tai arkkien molemmilla puolilla.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fi">Termi koskee kartografisia kuvia.</skos:scopeNote>
      <skos:prefLabel xml:lang="vi">cả hai mặt</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="vi">cả hai mặt</rdakit:toolkitLabel>
      <skos:definition xml:lang="vi">Sắp đặt được tiếp tục theo cùng tỷ lệ trên cả hai mặt của tờ hoặc các tờ.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="vi">Sắp đặt được tiếp tục theo cùng tỷ lệ trên cả hai mặt của tờ hoặc các tờ.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="vi">Thuật ngữ này áp dụng cho hình ảnh bản đồ.</skos:scopeNote>
      <skos:prefLabel xml:lang="da">begge sider</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">begge sider</rdakit:toolkitLabel>
      <skos:definition xml:lang="da">Layout der er fortsat i samme målestoksforhold på begge sider af et eller flere ark.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="da">Layout der er fortsat i samme målestoksforhold på begge sider af et eller flere ark.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="da">Denne term anvendes til kartografiske billeder.</skos:scopeNote>
      <skos:prefLabel xml:lang="fr">recto verso</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fr">recto verso</rdakit:toolkitLabel>
      <skos:definition xml:lang="fr">Disposition qui se poursuit à la même échelle sur les deux côtés d’une ou plusieurs feuilles.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fr">Disposition qui se poursuit à la même échelle sur les deux côtés d’une ou plusieurs feuilles.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="fr">Ce terme s’applique aux images cartographiques.</skos:scopeNote>
      <skos:prefLabel xml:lang="no">begge sider</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="no">begge sider</rdakit:toolkitLabel>
      <skos:definition xml:lang="no">Et oppsett som fortsetter i samme målestokk på gegge sider av ett eller flere ark.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="no">Et oppsett som fortsetter i samme målestokk på gegge sider av ett eller flere ark.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="no">Dette begrepet gjelder kartografiske bilder.</skos:scopeNote>
    </skos:Concept>
<!-- Concept: double line spacing  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1017" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8450"/>
      <skos:prefLabel xml:lang="en">double line spacing</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">double line spacing</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout containing a full blank line between lines of text.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout containing a full blank line between lines of text.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile text.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">interlinea doppia</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">interlinea doppia</rdakit:toolkitLabel>
      <skos:prefLabel xml:lang="de">doppelter Zeilenabstand</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="de">doppelter Zeilenabstand</rdakit:toolkitLabel>
      <skos:definition xml:lang="de">Ein Layout, das eine ganze Leerzeile zwischen den Zeilen eines Textes enthält.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="de">Ein Layout, das eine ganze Leerzeile zwischen den Zeilen eines Textes enthält.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="de">Dieser Terminus trifft auf taktilen Text zu.</skos:scopeNote>
      <skos:prefLabel xml:lang="fi">harvennettu riviväli</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">harvennettu riviväli</rdakit:toolkitLabel>

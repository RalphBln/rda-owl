      <skos:prefLabel xml:lang="fr">instrument de recherche</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fr">instrument de recherche</rdakit:toolkitLabel>
      <skos:definition xml:lang="fr">Instrument descriptif fournissant l’accès à une ressource.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fr">Instrument descriptif fournissant l’accès à une ressource.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="vi">hỗ trợ tìm kiếm</skos:prefLabel>

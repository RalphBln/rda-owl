      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8592"/>
      <skos:prefLabel xml:lang="en">resource</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">resource</rdakit:toolkitLabel>

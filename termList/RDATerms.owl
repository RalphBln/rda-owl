      <rdakit:toolkitDefinition xml:lang="it">Strumento descrittivo che fornisce accesso a una risorsa.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="de">Fundstellennachweis</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="de">Fundstellennachweis</rdakit:toolkitLabel>
      <skos:definition xml:lang="de">Eine Orientierungshilfe, die Zugang zu einer Ressource bietet.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="de">Eine Orientierungshilfe, die Zugang zu einer Ressource bietet.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="fi">hakemisto</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="fi">hakemisto</rdakit:toolkitLabel>
      <skos:definition xml:lang="fi">Kuvaileva väline joka tarjoaa pääsyn aineistoon.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="fi">Kuvaileva väline joka tarjoaa pääsyn aineistoon.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="da">vejledning</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="da">vejledning</rdakit:toolkitLabel>

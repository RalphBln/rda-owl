<!-- Concept: double leaf  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/RDATerms/1039" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/RDATerms"/>

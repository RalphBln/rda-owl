      <rdakit:toolkitDefinition xml:lang="vi">Công cụ mô tả cung cấp truy cập tới một tài nguyên.</rdakit:toolkitDefinition>
      <skos:prefLabel xml:lang="no">veiledning</skos:prefLabel>
